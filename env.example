# Digital Krishi Officer Frontend - Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API URL
# Point to your backend server (default: http://localhost:5000)
REACT_APP_API_URL=http://localhost:5000/api

# Default Location (optional - app will try to get user's location)
REACT_APP_DEFAULT_LAT=28.6139
REACT_APP_DEFAULT_LON=77.2090
REACT_APP_DEFAULT_CITY=New Delhi
